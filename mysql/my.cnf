[client]
port=3307
socket=/home/victor/wordpress-5.2.3-0/mysql/tmp/mysql.sock

default-character-set=UTF8
[manager]
port=3307
socket=/home/victor/wordpress-5.2.3-0/mysql/tmp/mysql.sock
pid-file=/home/victor/wordpress-5.2.3-0/mysql/tmp/manager.pid
default-mysqld-path=/home/victor/wordpress-5.2.3-0/mysql/bin/mysqld.bin

